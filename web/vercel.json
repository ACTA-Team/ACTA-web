{
  "rewrites": [
    {
      "source": "/api/testnet/:path*",
      "destination": "https://api.testnet.acta.build/:path*"
    },
    {
      "source": "/api/mainnet/:path*",
      "destination": "https://api.mainnet.acta.build/:path*"
    },
    {
      "source": "/links/:path*",
      "destination": "https://links.acta.build/:path*"
    }
  ]
}
